{
  "name": "froggy-friends-upgradeable",
  "version": "1.0.0",
  "description": "Froggy Friends upgradeable smart contracts",
  "main": "index.js",
  "scripts": {
    "bridge": "npx hardhat bridge --network mainnet --dst base --contract FroggyFriends",
    "compile": "npx hardhat compile",
    "size": "npx hardhat size-contracts",
    "airdrop": "npx hardhat run scripts/airdrop.ts --network mainnet",
    "deploy": "npx hardhat deploy --network mainnet --contract FroggyFriends",
    "test": "npx hardhat test --network hardhat --typecheck",
    "types": "npx hardhat typechain",
    "upgrade": "npx hardhat upgrade --network mainnet --contract FroggyFriends",
    "verify": "npx hardhat verify --network base 0x9DA02cBE93835E8b6c44563415C72D106B1ce00a"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Froggy-Friends/froggy-friends-upgradeable.git"
  },
  "author": "fonzy.eth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Froggy-Friends/froggy-friends-upgradeable/issues"
  },
  "homepage": "https://github.com/Froggy-Friends/froggy-friends-upgradeable#readme",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomicfoundation/hardhat-toolbox": "^2.0.2",
    "@nomicfoundation/hardhat-verify": "^2.0.4",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@openzeppelin/hardhat-upgrades": "^1.28.0",
    "@types/chai": "^4.3.5",
    "dotenv": "^10.0.0",
    "ethers": "^5.7.2",
    "hardhat": "^2.22.5",
    "hardhat-deploy": "^0.12.1"
  },
  "dependencies": {
    "@layerzerolabs/solidity-examples": "^0.0.13",
    "@openzeppelin/contracts-upgradeable": "^4.9.6",
    "hardhat-contract-sizer": "^2.10.0",
    "operator-filter-registry": "^1.4.2",
    "web3": "^4.9.0"
  }
}
